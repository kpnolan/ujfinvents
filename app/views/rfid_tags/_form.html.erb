<%= simple_form_for(@rfid_tag) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :tag_type %>
    <%= f.input :tag_id %>
    <%= f.association :plant %>
    <%= f.association :package %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
