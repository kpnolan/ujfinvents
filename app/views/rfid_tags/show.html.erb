<%- model_class = RfidTag -%>
<div class="page-header">
  <h1><%=t '.title', :default => RfidTag %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tag_type) %></dt>
      <dd><pre class="prettyprint"><%= @rfid_tag.tag_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:tag_id) %></dt>
      <dd><pre class="prettyprint"><%= @rfid_tag.tag_id %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              rfid_tags_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_rfid_tag_path(@rfid_tag), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              rfid_tag_path(@rfid_tag),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

